id: mbedtls_cmac
package: platform
label: Mbed TLS support for CMAC
description: This component provides support for CMAC using Mbed TLS
category: Platform|Security
quality: production
source:
  - path: util/third_party/mbedtls/library/cmac.c
include:
  - path: util/third_party/mbedtls/include
    file_list:
      - path: mbedtls/cmac.h
provides:
  - name: mbedtls_cmac
requires:
  - name: mbedtls_aes
  - name: mbedtls_cipher
  - name: mbedtls_core
template_contribution:
  - name: mbedtls_config
    value: MBEDTLS_CMAC_C
documentation:
  docset: mbed-tls
  document: group-sl-crypto-cmac
